{
  "custom_assertions_path": "./examples/custom-assertions",
  "custom_commands_path": "./examples/custom-commands",
  "disable_colors": false,
  "globals_path": "./examples/globalsModule.js",
  "output_folder": "./examples/reports",
  "page_objects_path": "./examples/pages",
  "src_folders": [
    "./examples/tests",
    "./examples/mocha",
    "./examples/unittests"
  ],
  "test_settings": {
    "chrome": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "loggingPrefs": {
          "browser": "INFO",
          "driver": "INFO",
          "server": "OFF"
        }
      },
      "webdriver": {
        "cli_args": [
          "--verbose"
        ],
        "default_path_prefix": "",
        "port": 9515,
        "server_path": "./bin/chromedriver-2.32"
      }
    },
    "default": {
      "desiredCapabilities": {
        "acceptInsecureCerts": true,
        "browserName": "firefox"
      },
      "disable_colors": false,
      "filter": [
        "./examples/tests"
      ],
      "request_timeout_options": {
        "retry_attempts": 5,
        "timeout": 60000
      },
      "screenshots": {
        "enabled": false,
        "path": ""
      },
      "webdriver": {
        "cli_args": [
          "--log",
          "debug"
        ],
        "server_path": "./bin/geckodriver-0.20"
      }
    },
    "mocha": {
      "desiredCapabilities": {
        "browserName": "chrome"
      },
      "test_runner": {
        "type": "mocha"
      },
      "webdriver": {
        "cli_args": [
          "--verbose"
        ],
        "default_path_prefix": "",
        "server_path": "./bin/chromedriver-2.32"
      }
    },
    "selenium_server": {
      "desiredCapabilities": {
        "acceptSslCerts": true,
        "browserName": "firefox"
      },
      "selenium": {
        "cli_args": {
          "webdriver.chrome.driver": "./bin/chromedriver-2.32",
          "webdriver.gecko.driver": "./bin/geckodriver-0.19"
        },
        "host": "localhost",
        "port": 4444,
        "server_path": "./bin/selenium-server-standalone-3.10.0.jar",
        "start_process": true
      }
    },
    "unit_tests": {
      "exclude": "",
      "filter": "./examples/unittests/*",
      "unit_tests_mode": true
    }
  },
  "test_workers": false,
  "webdriver": {
    "host": "localhost",
    "port": 4444,
    "start_process": false
  }
}