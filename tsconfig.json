{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "paths": {
      "@evan.network/digital-twin-detail": [
        "./dapps/digital-twin-detail"
      ],
      "@evan.network/digital-twin-lib": [
        "./core/digital-twin-lib"
      ],
      "@evan.network/ui": [
        "./core/ui.libs"
      ],
      "@evan.network/ui-countries": [
        "./libs/countries.libs"
      ],
      "@evan.network/ui-session": [
        "./core/ui-session"
      ],
      "@evan.network/ui-vue-core": [
        "./core/evancore.vue.libs"
      ],
      "@evan.network/profile": [
        "./dapps/profile.vue"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "target": "es6"
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "files": [
    "./scripts/dapp/vue-shim.d.ts"
  ],
  "include": [
    "./src/**/*",
    "./src/**/*.json"
  ]
}
