{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "paths": {
      "@evan.network/ui": ["./core/ui.libs"],
      "@evan.network/ui/*": ["./core/ui.libs/*"],
      "@evan.network/ui-vue-core": ["./core/evancore.vue.libs"],
      "@evan.network/digitaltwin.lib": ["./dapps/digital-twin.lib"],
      "@evan.network/twin-detail": ["./dapps/evan-twin-detail"],
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "target": "es5"
  },
  "exclude": ["node_modules"],
  "files": ["./scripts/dapp/vue-shim.d.ts"]
}
