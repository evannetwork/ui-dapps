{
  "public": {
    "author": "evan GmbH",
    "dapp": {
      "dependencies": {
        "angular-core": "^2.2.1",
        "angular-libs": "^2.1.1"
      },
      "entrypoint": "task.js",
      "files": [
        "task.css",
        "task.js"
      ],
      "origin": "QmQTEKeaihnXD2VRTRLginGU47FqC4qBXkb1MB3srPSXjm",
      "primaryColor": "#e87e23",
      "secondaryColor": "#fffaf5",
      "standalone": true,
      "type": "dapp"
    },
    "dataSchema": {
      "metadata": {
        "$id": "metadata_schema",
        "additionalProperties": true,
        "properties": {
          "bc": {
            "type": "string"
          },
          "created": {
            "type": "number"
          },
          "createdby": {
            "type": "string"
          },
          "strictFinish": {
            "type": "boolean"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "todologs": {
        "$id": "todologs_schema",
        "additionalProperties": false,
        "properties": {
          "choice": {
            "type": "boolean"
          },
          "comment": {
            "type": "string"
          },
          "files": {
            "type": "object"
          },
          "id": {
            "type": "string"
          },
          "pictures": {
            "type": "string"
          },
          "signature": {
            "type": "string"
          },
          "signatureClear": {
            "type": "string"
          },
          "solveTime": {
            "type": "number"
          },
          "solver": {
            "type": "string"
          },
          "solverAlias": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "solveTime",
          "solver"
        ],
        "type": "object"
      },
      "todos": {
        "$id": "todos_schema",
        "additionalProperties": false,
        "properties": {
          "alias": {
            "type": "string"
          },
          "created": {
            "type": "number"
          },
          "createdFrom": {
            "type": "string"
          },
          "criteria": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "id": {
            "type": "string"
          },
          "labels": {
            "properties": {
              "choice": {
                "type": "string"
              },
              "comment": {
                "type": "string"
              },
              "files": {
                "type": "string"
              },
              "pictures": {
                "type": "string"
              },
              "signature": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "order": {
            "type": "number"
          },
          "required": {
            "properties": {
              "choice": {
                "type": "boolean"
              },
              "comment": {
                "type": "boolean"
              },
              "files": {
                "type": "number"
              },
              "pictures": {
                "type": "number"
              },
              "signature": {
                "type": "boolean"
              }
            },
            "type": "object"
          }
        },
        "required": [
          "alias",
          "criteria",
          "id",
          "labels",
          "order",
          "required"
        ],
        "type": "object"
      }
    },
    "dbcpVersion": 2,
    "description": "Create todos and manage updates.",
    "i18n": {
      "description": {
        "de": "Erstelle Aufgaben oder zeige sie an",
        "en": "Create tasks or show them"
      },
      "name": {
        "de": "Task",
        "en": "Task"
