{
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "flatModuleId": "favorites",
    "flatModuleOutFile": "favorites.js",
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true
  },
  "compilerOptions": {
    "baseUrl": "./src",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../build",
    "paths": {
      "angular-core": [
        "../../../node_modules/@evan.network/ui-angular-core/dist/angularcore.js"
      ],
      "angular-libs": [
        "../../../node_modules/@evan.network/ui-angular-libs/dist/angularlibs.js"
      ],
      "bcc": [
        "../../../node_modules/@evan.network/api-blockchain-core/dist/index.js"
      ],
      "dapp-browser": [
        "../../../node_modules/@evan.network/ui-dapp-browser/runtime/build/main"
      ],
      "smart-contracts": [
        "../../../node_modules/@evan.network/smart-contracts-core/index.js"
      ]
    },
    "rootDir": "./src",
    "skipLibCheck": true,
    "stripInternal": true,
    "target": "es5",
    "types": []
  },
  "files": [
    "./index.ts"
  ]
}