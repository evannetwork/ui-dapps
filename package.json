  "scripts": {
    "browserify": "gulp --gulpfile scripts/gulp/browserify.js broawserify --dapp ",
    "build": "npm run dapps-build",
    "clear-build": "npm run link-core && rm -r ./node_modules/@evan.network/ui-dapp-browser/runtime/external/*",
    "dapp-build": "npm run link-core && gulp --gulpfile scripts/dapps.js dapps-build --folder ",
    "dapps-build": "npm run link-core && gulp --gulpfile scripts/dapps.js dapps-build",
    "dapps-serve": "npm run link-core && gulp --gulpfile scripts/dapps.js dapps-serve",
    "deploy": "node --inspect node_modules/.bin/gulp --gulpfile node_modules/@evan.network/ui-dapp-browser/gulp/deployment.js deploy --advanced --config",
    "deploy-mainnet": "npm run deploy $(pwd)/../../dev-environment/config/bcc/mainnet.js",
    "deploy-testnet": "npm run deploy $(pwd)/../../dev-environment/config/bcc/testnet.js",
    "link-core": "gulp --gulpfile scripts/link-core.js link-core",
    "serve": "npm run link-core && gulp --gulpfile scripts/dapps.js serve",
    "serve-build": "npm run link-core && gulp --gulpfile node_modules/@evan.network/ui-dapp-browser/gulp/serve.js serve --build",
    "test-local": "cd ./tests-e2e && env-cmd -f .env.local cucumber-js --require-module @babel/register --require cucumber.conf.js --require step-definitions --format ../node_modules/cucumber-pretty",
    "test-local-only": "cd ./tests-e2e && env-cmd -f .env.local cucumber-js --tags '@only' --require-module @babel/register --require cucumber.conf.js --require step-definitions --format ../node_modules/cucumber-pretty",
    "test-testnet": "cd ./tests-e2e && env-cmd cucumber-js --require-module @babel/register --require cucumber.conf.js --require step-definitions --format ../node_modules/cucumber-pretty",
    "test-testnet-only": "cd ./tests-e2e && env-cmd cucumber-js --tags '@only' --require-module @babel/register --require cucumber.conf.js --require step-definitions --format ../node_modules/cucumber-pretty",
    "test-testnet-specific": "cd ./tests-e2e && env-cmd cucumber-js --require-module @babel/register --require cucumber.conf.js --require step-definitions --format ../node_modules/cucumber-pretty ${TESTPATTERN}",
    "tsc": "tsc -p "
  },
